<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd"
	default-autowire="byName">

    <bean name="layerMenu"
		class="org.geosdi.geoplatform.gui.configuration.menubar.CheckMenuClientTool">
        <property name="id" value="layerMenu" />
        <property name="text" value="Layers" />
        <property name="checked" value="true" />
        <property name="enabled" value="true" />
        <property name="order" value="30" />
    </bean>
    
     <bean name="ZoomToLayerExtent"
		class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="ZoomToLayerExtent" />
        <property name="text" value="Zoom to layer extent" />
        <property name="enabled" value="true" />
        <property name="order" value="40" />
    </bean>
    
    <bean name="exportToKML"
		class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="exportToKML" />
        <property name="text" value="Export To KML" />
        <property name="enabled" value="true" />
        <property name="order" value="50" />
    </bean>
    
    <bean name="exportToPDF"
		class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="exportToPDF" />
        <property name="text" value="Export To PDF" />
        <property name="enabled" value="true" />
        <property name="order" value="60" />
    </bean>
    
    <bean name="exportToTIFF"
		class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="exportToTIFF" />
        <property name="text" value="Export To Tiff" />
        <property name="enabled" value="true" />
        <property name="order" value="70" />
    </bean>
    
    <bean name="exportToShpZip"
		class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="exportToShpZip" />
        <property name="text" value="Export To Shape-Zip" />
        <property name="enabled" value="true" />
        <property name="order" value="80" />
    </bean>
    
    <bean name="LayerProperties"
		class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="LayerProperties" />
        <property name="text" value="Layer Properties" />
        <property name="enabled" value="true" />
        <property name="order" value="90" />
    </bean>
    

</beans>